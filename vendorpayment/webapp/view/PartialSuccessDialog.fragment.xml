<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
>
    <Dialog
        id="_IDGenDialog1"
        title="{partial>/title}"
        contentWidth="70%"
        contentHeight="80%"
        resizable="true"
        draggable="true"
    >
        <VBox
            id="_IDGenVBox5"
            class="sapUiSmallMargin"
        >
            <Title
                id="_IDGenTitle2"
                text="{partial>/message}"
                class="sapUiSmallMarginBottom"
            />

            <IconTabBar
                id="_IDGenIconTabBar"
                expanded="true"
                class="sapUiResponsiveContentPadding"
            >
                <items>
                    <IconTabFilter
                        id="_IDGenIconTabFilter"
                        icon="sap-icon://document"
                        iconColor="Positive"
                        text="Successful Documents ({partial>/successCount})"
                        key="documents"
                    >
                        <Table
                            id="_IDGenTable2"
                            items="{partial>/documents}"
                            fixedLayout="false"
                            alternateRowColors="true"
                            growing="true"
                            growingThreshold="10"
                            growingScrollToLoad="true"
                        >
                            <columns>
                                <Column id="_IDGenColumn">
                                    <Text
                                        id="_IDGenText28"
                                        text="Sequence ID"
                                    />
                                </Column>
                                <Column id="_IDGenColumn6">
                                    <Text
                                        id="_IDGenText29"
                                        text="Document"
                                    />
                                </Column>
                                <Column
                                    id="_IDGenColumn7"
                                    minScreenWidth="Tablet"
                                    demandPopin="true"
                                >
                                    <Text
                                        id="_IDGenText30"
                                        text="Company Code"
                                    />
                                </Column>
                                <Column
                                    id="_IDGenColumn8"
                                    minScreenWidth="Tablet"
                                    demandPopin="true"
                                >
                                    <Text
                                        id="_IDGenText31"
                                        text="Fiscal Year"
                                    />
                                </Column>
                                <Column
                                    id="_IDGenColumn9"
                                    width="30%"
                                >
                                    <Text
                                        id="_IDGenText32"
                                        text="Status Message"
                                    />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem
                                    id="_IDGenColumnListItem2"
                                    type="Active"
                                    press=".onDocumentItemPress"
                                >
                                    <cells>
                                        <Text
                                            id="_IDGenText33"
                                            text="{partial>sequenceId}"
                                        />
                                        <ObjectIdentifier
                                            id="_IDGenObjectIdentifier1"
                                            title="{partial>documentNumber}"
                                            text="{partial>formattedDocNumber}"
                                        />
                                        <Text
                                            id="_IDGenText34"
                                            text="{partial>companyCode}"
                                        />
                                        <Text
                                            id="_IDGenText35"
                                            text="{partial>fiscalYear}"
                                        />
                                        <Text
                                            id="_IDGenText36"
                                            text="{partial>message}"
                                        />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </IconTabFilter>

                    <IconTabFilter
                        id="_IDGenIconTabFilter1"
                        icon="sap-icon://error"
                        iconColor="Negative"
                        text="Failed Entries ({partial>/failedCount})"
                        key="errors"
                    >
                        <Table
                            id="_IDGenTable3"
                            items="{partial>/errors}"
                            fixedLayout="false"
                            alternateRowColors="true"
                            growing="true"
                            growingThreshold="10"
                            growingScrollToLoad="true"
                        >
                            <columns>
                                <Column id="_IDGenColumn10">
                                    <Text
                                        id="_IDGenText37"
                                        text="Sequence ID"
                                    />
                                </Column>
                                <Column
                                    id="_IDGenColumn11"
                                    minScreenWidth="Tablet"
                                    demandPopin="true"
                                >
                                    <Text
                                        id="_IDGenText38"
                                        text="Status Code"
                                    />
                                </Column>
                                <Column id="_IDGenColumn12">
                                    <Text
                                        id="_IDGenText39"
                                        text="Status Message"
                                    />
                                </Column>
                                <Column
                                    id="_IDGenColumn13"
                                    width="40%"
                                >
                                    <Text
                                        id="_IDGenText40"
                                        text="Error Details"
                                    />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem id="_IDGenColumnListItem3">
                                    <cells>
                                        <Text
                                            id="_IDGenText41"
                                            text="{partial>sequenceId}"
                                        />
                                        <Text
                                            id="_IDGenText42"
                                            text="{partial>statusCode}"
                                        />
                                        <Text
                                            id="_IDGenText43"
                                            text="{partial>statusMessage}"
                                        />
                                        <List
                                            id="_IDGenList1"
                                            items="{partial>errorDetails}"
                                            showNoData="false"
                                        >
                                            <items>
                                                <StandardListItem
                                                    id="_IDGenStandardListItem3"
                                                    title="{}"
                                                    type="Inactive"
                                                />
                                            </items>
                                        </List>
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </VBox>

        <buttons>            
            <Button
                id="_IDGenButton11"
                text="Export to Excel"
                icon="sap-icon://excel-attachment"
                press=".onExportData"
            >
                <customData>
                    <core:CustomData
                        key="format"
                        value="xlsx"
                    />
                    <core:CustomData
                        key="type"
                        value="partial"
                    />
                </customData>
            </Button>
            <Button
                id="_IDGenButton12"
                text="Export to CSV"
                icon="sap-icon://attachment-text-file"
                press=".onExportData"
            >
                <customData>
                    <core:CustomData
                        key="format"
                        value="csv"
                    />
                    <core:CustomData
                        key="type"
                        value="partial"
                    />
                </customData>
            </Button>
            <Button
                id="_IDGenButton13"
                text="Export to PDF"
                icon="sap-icon://pdf-attachment"
                press=".onExportData"
            >
                <customData>
                    <core:CustomData
                        key="format"
                        value="pdf"
                    />
                    <core:CustomData
                        key="type"
                        value="partial"
                    />
                </customData>
            </Button>
            <Button
                id="_IDGenButton14"
                text="Close"
                press=".onPartialSuccessDialogClose"
            />
        </buttons>
    </Dialog>
</core:FragmentDefinition>

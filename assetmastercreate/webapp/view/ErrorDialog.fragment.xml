<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
>
    <Dialog
        id="_IDGenDialog2"
        title="{error>/title}"
        contentWidth="50rem"
        resizable="true"
        draggable="true"
    >
        <content>
            <VBox
                id="_IDGenVBox6"
                class="sapUiSmallMargin"
            >
                <MessageStrip
                    id="_IDGenMessageStrip1"
                    text="{error>/message}"
                    type="Error"
                    showIcon="true"
                    class="sapUiSmallMarginBottom"
                />

                <Table
                    id="_IDGenTable1"
                    items="{error>/errors}"
                    fixedLayout="false"
                    width="100%"
                >
                    <columns>
                        <Column
                            id="_IDGenColumn14"
                            width="6rem"
                        >
                            <Text
                                id="_IDGenText44"
                                text="Sequence"
                            />
                        </Column>
                        <Column
                            id="_IDGenColumn15"
                            width="6rem"
                        >
                            <Text
                                id="_IDGenText45"
                                text="Status Code"
                            />
                        </Column>
                        <Column id="_IDGenColumn16">
                            <Text
                                id="_IDGenText46"
                                text="Error Details"
                            />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem id="_IDGenColumnListItem4">
                            <cells>
                                <Text
                                    id="_IDGenText47"
                                    text="{error>sequenceId}"
                                />
                                <Text
                                    id="_IDGenText48"
                                    text="{error>statusCode}"
                                />
                                <VBox id="_IDGenVBox7">
                                    <Text
                                        id="_IDGenText49"
                                        text="{error>statusMessage}"
                                    />
                                    <List
                                        id="_IDGenList2"
                                        visible="{= ${error>errorDetails}.length > 0 }"
                                        showSeparators="None"
                                        items="{error>errorDetails}"
                                    >
                                        <items>
                                            <StandardListItem
                                                id="_IDGenStandardListItem4"
                                                title="{error>}"
                                                type="Inactive"
                                            />
                                        </items>
                                    </List>
                                </VBox>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </VBox>
        </content>
        <buttons>
            <Button
                id="_IDGenButton16"
                text="Export to Excel"
                icon="sap-icon://excel-attachment"
                press="onExportData"
            >
                <customData>
                    <core:CustomData
                        key="format"
                        value="xlsx"
                    />
                    <core:CustomData
                        key="type"
                        value="error"
                    />
                </customData>
            </Button>
            <Button
                id="_IDGenButton17"
                text="Export to CSV"
                icon="sap-icon://attachment-text-file"
                press="onExportData"
            >
                <customData>
                    <core:CustomData
                        key="format"
                        value="csv"
                    />
                    <core:CustomData
                        key="type"
                        value="error"
                    />
                </customData>
            </Button>
            <Button
                id="_IDGenButton18"
                text="Export to PDF"
                icon="sap-icon://pdf-attachment"
                press="onExportData"
            >
                <customData>
                    <core:CustomData
                        key="format"
                        value="pdf"
                    />
                    <core:CustomData
                        key="type"
                        value="error"
                    />
                </customData>
            </Button>
            <Button
                id="closeErrorDialogButton"
                text="Close"
                press=".onCloseErrorDialog"
                type="Emphasized"
            />
        </buttons>
    </Dialog>
</core:FragmentDefinition>

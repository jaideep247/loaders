<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        id="helpDialog"
        title="WBS Element Excel Upload Help"
        contentWidth="600px"
    >
        <content>
            <IconTabBar
                id="helpTabBar"
                expanded="true"
            >
                <items>
                    <IconTabFilter
                        id="gettingStartedTab"
                        icon="sap-icon://hint"
                        text="Getting Started"
                    >
                        <content>
                            <VBox
                                id="_IDGenVBox12"
                                class="sapUiSmallMarginBegin sapUiSmallMarginEnd"
                            >
                                <items>
                                    <Title
                                        id="_IDGenTitle3"
                                        text="How to Upload WBS Elements"
                                        class="sapUiSmallMarginTop"
                                    />
                                    <Text
                                        id="_IDGenText8"
                                        text="1. Download the template using the 'Download Template' button."
                                        class="sapUiTinyMarginTop"
                                    />
                                    <Text
                                        id="_IDGenText12"
                                        text="2. Fill in the template with your WBS element data."
                                    />
                                    <Text
                                        id="_IDGenText13"
                                        text="3. Upload the completed file using the 'Choose File' button."
                                    />
                                    <Text
                                        id="_IDGenText14"
                                        text="4. Review validation results and fix any errors."
                                    />
                                    <Text
                                        id="_IDGenText15"
                                        text="5. Click 'Submit WBS Elements' to process valid entries."
                                    />
                                </items>
                            </VBox>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter
                        id="commonErrorsTab"
                        icon="sap-icon://alert"
                        text="Common Errors"
                    >
                        <content>
                            <List id="_IDGenList1">
                                <items>
                                    <StandardListItem
                                        id="_IDGenStandardListItem1"
                                        title="Missing Project UUID"
                                        description="Parent Project UUID is required for each WBS element."
                                    />
                                    <StandardListItem
                                        id="_IDGenStandardListItem3"
                                        title="Missing Required Fields"
                                        description="All required fields must be filled in for each entry."
                                    />
                                    <StandardListItem
                                        id="_IDGenStandardListItem5"
                                        title="Invalid Date Format"
                                        description="Dates should be in YYYY-MM-DD format."
                                    />
                                    <StandardListItem
                                        id="_IDGenStandardListItem6"
                                        title="Duplicate Project Element ID"
                                        description="Project Element ID must be unique within the system."
                                    />
                                    <StandardListItem
                                        id="_IDGenStandardListItem7"
                                        title="Invalid Parent Project"
                                        description="The specified Project UUID must exist in the system."
                                    />
                                </items>
                            </List>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter
                        id="fileFormatTab"
                        icon="sap-icon://attachment-text-file"
                        text="File Format"
                    >
                        <content>
                            <VBox
                                id="_IDGenVBox13"
                                class="sapUiSmallMarginBegin sapUiSmallMarginEnd"
                            >
                                <items>
                                    <Text
                                        id="_IDGenText16"
                                        text="The Excel file should contain the following columns:"
                                        class="sapUiSmallMarginTop"
                                    />
                                    <Panel
                                        id="_IDGenPanel5"
                                        headerText="Required Fields"
                                        expandable="true"
                                        expanded="true"
                                    >
                                        <content>
                                            <List id="_IDGenList3">
                                                <items>
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem8"
                                                        title="ProjectElement: Project Element ID"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem9"
                                                        title="ProjectUUID: Parent Project UUID"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem10"
                                                        title="ProjectElementDescription: Name/Description of element"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem11"
                                                        title="PlannedStartDate: Planned start date (YYYY-MM-DD)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem12"
                                                        title="PlannedEndDate: Planned finish date (YYYY-MM-DD)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem13"
                                                        title="ResponsibleCostCenter: Responsible cost center"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem14"
                                                        title="CompanyCode: Company code"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem15"
                                                        title="ProfitCenter: Profit center"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem16"
                                                        title="ControllingArea: Controlling area"
                                                    />
                                                </items>
                                            </List>
                                        </content>
                                    </Panel>
                                    <Panel
                                        id="_IDGenPanel6"
                                        headerText="Optional Fields"
                                        expandable="true"
                                        expanded="false"
                                        class="sapUiTinyMarginTop"
                                    >
                                        <content>
                                            <List id="_IDGenList4">
                                                <items>
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem17"
                                                        title="WBSElementIsBillingElement: Billing element indicator (X/blank)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem18"
                                                        title="ParentObjectUUID: Parent WBS Element UUID (hierarchical)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem19"
                                                        title="YY1_OldProjectSiteID_PTD: Old Project ID"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem20"
                                                        title="YY1_ExactWBScode_PTD: Exact WBS code"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem21"
                                                        title="YY1_Categorization1_PTD: Site Type (OF/ON)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem22"
                                                        title="YY1_ATMID_PTD: ATM ID"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem23"
                                                        title="YY1_Project_PTD: Bank Name"
                                                    />
                                                </items>
                                            </List>
                                        </content>
                                    </Panel>
                                    <Panel
                                        id="_IDGenPanel8"
                                        headerText="More Custom Fields"
                                        expandable="true"
                                        expanded="false"
                                        class="sapUiTinyMarginTop"
                                    >
                                        <content>
                                            <List id="_IDGenList6">
                                                <items>
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem29"
                                                        title="YY1_Address_PTD: District"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem30"
                                                        title="YY1_State_PTD: State"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem31"
                                                        title="YY1_ATMCount_PTD: ATM Count"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem32"
                                                        title="YY1_NatureOfWBS_PTD: Nature Of WBS (Site/ATM)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem33"
                                                        title="YY1_SAPsiteIDReport_PTD: SAP site ID (Report)"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem34"
                                                        title="YY1_Addressandpostalco_PTD: Address and postal code"
                                                    />
                                                    <StandardListItem
                                                        id="_IDGenStandardListItem35"
                                                        title="YY1_Postalcode_PTD: Postal code"
                                                    />
                                                </items>
                                            </List>
                                        </content>
                                    </Panel>
                                    <Link
                                        id="_IDGenLink"
                                        text="Download Sample Template"
                                        press=".onDownloadTemplate"
                                        class="sapUiSmallMarginTop"
                                    />
                                </items>
                            </VBox>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter
                        id="processingRulesTab"
                        icon="sap-icon://process"
                        text="Processing Rules"
                    >
                        <content>
                            <VBox
                                id="_IDGenVBox14"
                                class="sapUiSmallMarginBegin sapUiSmallMarginEnd"
                            >
                                <items>
                                    <Title
                                        id="_IDGenTitle4"
                                        text="WBS Element Processing Rules"
                                        class="sapUiSmallMarginTop"
                                    />
                                    <Text
                                        id="_IDGenText17"
                                        text="Each WBS element must follow these rules:"
                                        class="sapUiTinyMarginTop"
                                    />
                                    <List id="_IDGenList5">
                                        <items>
                                            <StandardListItem
                                                id="_IDGenStandardListItem24"
                                                title="Valid Project Reference"
                                                description="Project UUID must reference an existing project in the system."
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem25"
                                                title="Valid Dates"
                                                description="Planned start date must be before or equal to planned end date."
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem26"
                                                title="Valid Organizational Units"
                                                description="Company code, controlling area, cost center, and profit center must exist in the system."
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem27"
                                                title="Hierarchy Consistency"
                                                description="If Parent Object UUID is specified, it must reference a valid parent WBS element."
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem28"
                                                title="Unique Identifier"
                                                description="Project Element ID must be unique within the system."
                                            />
                                        </items>
                                    </List>
                                </items>
                            </VBox>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter
                        id="customFieldsTab"
                        icon="sap-icon://customize"
                        text="Banking Fields"
                    >
                        <content>
                            <VBox
                                id="_IDGenVBox15"
                                class="sapUiSmallMarginBegin sapUiSmallMarginEnd"
                            >
                                <items>
                                    <Title
                                        id="_IDGenTitle6"
                                        text="Banking Industry Specific Fields"
                                        class="sapUiSmallMarginTop"
                                    />
                                    <Text
                                        id="_IDGenText19"
                                        text="The following custom fields are available for banking industry use cases:"
                                        class="sapUiTinyMarginTop"
                                    />
                                    <List id="_IDGenList7">
                                        <items>
                                            <StandardListItem
                                                id="_IDGenStandardListItem36"
                                                title="YY1_Deployment_PTD"
                                                description="Deployment type (e.g., URBAN, RURAL)"
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem37"
                                                title="YY1_BankLoadATMDiscoun_PTD"
                                                description="Bank Load ATM Discount Percent"
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem38"
                                                title="YY1_ERPRelocationRefAT_PTD"
                                                description="ERP Relocation Reference ATM ID"
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem39"
                                                title="YY1_ERPsiteIDReport_PTD"
                                                description="ERP site ID for reporting"
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem40"
                                                title="YY1_Categorization_PTD"
                                                description="Categorization (e.g., GOLD, SILVER)"
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem41"
                                                title="YY1_UDF1_PTD, YY1_UDF2_PTD, YY1_UDF3_PTD"
                                                description="User-defined fields for additional information"
                                            />
                                            <StandardListItem
                                                id="_IDGenStandardListItem42"
                                                title="YY1_ERPRelocationRefer_PTD"
                                                description="ERP Relocation Reference Site ID"
                                            />
                                        </items>
                                    </List>
                                </items>
                            </VBox>
                        </content>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </content>
        <beginButton>
            <Button
                id="_IDGenButton13"
                text="Close"
                press=".onHelpDialogClose"
            />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
